{
  "name": "users",
  "version": "0.0.1",
  "description": "n4b project's users services",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aucune-idee/users.git"
  },
  "main": "app/server.ts",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "rimraf build/ && tsc && cp app.yaml build/ && cp package.json build/ && cp app/secret.json build/app",
    "dev": "ts-node-dev --respawn --transpileOnly ./app/server.ts",
    "prod": "tsc && node ./build/server.js",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "start": "node ./app/server.js"
  },
  "keywords": [
    "n4b",
    "service",
    "user",
    "users"
  ],
  "author": "Kamule <benjamin.herbomez@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aucune-idee/users/issues"
  },
  "homepage": "https://github.com/aucune-idee/users#readme",
  "dependencies": {
    "body-parse": "^0.1.0",
    "email-validator": "^2.0.4",
    "express": "^4.16.4",
    "express-promise-router": "^3.0.3",
    "js-sha512": "^0.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.2",
    "mongoose-auto-increment": "^5.0.1",
    "mongoose-auto-increment-reworked": "^1.2.1",
    "morgan": "^1.9.1",
    "typescript": "^3.4.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/express-promise-router": "^2.0.1",
    "@types/jest": "^24.0.11",
    "@types/js-sha512": "^0.7.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mongodb": "^3.1.24",
    "@types/mongoose": "^5.3.26",
    "@types/mongoose-auto-increment": "^5.0.30",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.13.7",
    "@types/passport": "^1.0.0",
    "coveralls": "^3.0.3",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "^1.0.0-pre.32"
  }
}
